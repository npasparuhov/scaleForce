{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react-hooks"
  ],
  "settings": {
    "react": {
      "createClass": "createReactClass",
      "pragma": "React",
      "version": "detect",
      "flowVersion": "0.53"
    }
  },
  "rules": {
    "no-cond-assign": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-keys": 2,
    "no-empty": 2,
    "no-console": 0,
    "no-empty-character-class": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "block-scoped-var": 2,
    "consistent-return": 2,
    "curly": [
      2,
      "multi"
    ],
    "default-case": 2,
    "dot-notation": 2,
    "no-caller": 2,
    "no-confusing-arrow": [
      "error",
      {
        "allowParens": true
      }
    ],
    "no-div-regex": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-implied-eval": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-octal": 2,
    "no-octal-escape": 0,
    "no-useless-escape": 0,
    "no-process-env": 0,
    "no-unused-vars": 0,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-warning-comments": 2,
    "no-with": 2,
    "prefer-arrow-callback": 2,
    "no-var": 2,
    "no-undef": 0,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "arrow-body-style": [
      2,
      "as-needed"
    ],
    "arrow-parens": [
      2,
      "as-needed"
    ],
    "arrow-spacing": 2,
    "brace-style": 2,
    "comma-spacing": 2,
    "comma-style": 2,
    "eol-last": 2,
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "new-parens": 2,
    "no-array-constructor": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-new-object": 2,
    "semi-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "no-spaced-func": 2,
    "no-trailing-spaces": 2,
    "no-multiple-empty-lines": 2,
    "operator-assignment": [
      2,
      "always"
    ],
    "quotes": [
      2,
      "single"
    ],
    "quote-props": [
      2,
      "as-needed"
    ],
    "semi": [
      2,
      "always"
    ],
    "sort-vars": [
      2,
      {
        "ignoreCase": true
      }
    ],
    "keyword-spacing": 2,
    "space-before-blocks": 2,
    "object-curly-spacing": [
      2,
      "always"
    ],
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "max-len": [
      2,
      150,
      {
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true,
        "ignoreComments": true
      }
    ],
    "no-prototype-builtins": 2,
    "react-hooks/rules-of-hooks": 0,
    "react/display-name": 0,
    "react/prop-types": 0
  }
}
